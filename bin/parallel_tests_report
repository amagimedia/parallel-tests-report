#!/usr/bin/env ruby

gem_dir = File.expand_path("..",File.dirname(__FILE__))
$LOAD_PATH.unshift gem_dir
exec_type = ARGV[0]
if exec_type == 'rake' then
    require 'rake'
    require 'pp'
    pwd=Dir.pwd
    Dir.chdir(gem_dir)
    Rake.application.init
    Rake.application.load_rakefile
    Dir.chdir(pwd)
    Rake.application.invoke_task(ARGV[1])
end
